<odoo>
	<record id="view_form_edu_demo_model" model="ir.ui.view">
		<field name="name">edu.demoplo.model.form</field>
		<field name="model">edu.demoplo.model</field>
		<field name="arch" type="xml">
			<form>
				<sheet>
					<group>
						<field name="name"/>
					</group>
				</sheet>
			</form>
		</field>
	</record>

	<record id="action_edu_demo_model" model="ir.actions.act_window">
		<field name="name">Demo Model</field>
		<field name="res_model">edu.demoplo.model</field>
		<field name="view_mode">form</field>
	</record>

	<menuitem id="menu_edu_demo_root" name="Edu Demoplo"/>
	<menuitem     id="menu_edu_demo_model" name="Demo Model" parent="menu_edu_demo_root" action="action_edu_demo_model"/>
</odoo>
